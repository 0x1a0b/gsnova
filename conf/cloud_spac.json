[
  {
    "Host" : ["*.ytimg.com*", "*.google.com(.*)?","*.googleusercontent.com*", "*.gstatic.com", "*.googleapis.com*", "*.googlecode.com"],
    "Proxy":["GoogleHttps", "Direct", "Default"]
  },
  {
    "Host" : ["*.doubleclick.net*", "*.google-analytics.com*", "*.blogger.com", "*.google.co.*"],
    "Proxy":["GoogleHttps", "Direct", "Default"]
  },
  {
    "Host" : ["*.golang.org"],
    "Proxy": ["Direct", "Default"]
  },
  {
    "Host" : ["*.blogspot.*", "*.appspot.com", "golang.org"],
    "Proxy": ["GoogleHttps", "Direct", "Default"]
  },
  {
    "Host" : ["*.youtube.com"],
    "Proxy": ["GoogleHttps", "Direct", "Default"],
    "Attr":  ["Direct", "Default"]
  },
  {
    "Protocol" : "http",
    "Host": ["twitter.com", "www.facebook.com"],
    "Attr":  ["RedirectHttps"]
  },
  {
    "Protocol" : "https",
    "Host": ["twitter.com"],
    "Proxy": ["Default"]
  },
  {
    "Filter" : ["!IsBlockedByGFW"],
    "Proxy":["Direct", "Default"]
  }
]
